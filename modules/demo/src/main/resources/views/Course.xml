<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">
    <grid name="course-grid" title="课程" model="com.yyc.demo.db.Course">
        <field name="school" title="学校"/>
        <field name="subject" title="学科"/>
        <field name="teacher" title="教师"/>
    </grid>

    <form name="course-form" title="课程" model="com.yyc.demo.db.Course">
        <panel name="mainPanel" >
            <panel title="课程基本信息">
                <field name="school"/>
                <field name="subject"/>
                <field name="teacher"/>
            </panel>
        </panel>
        <panel-tabs>
            <panel-related title="参与学生" field="student" editable="true" orderBy="sequence" canMove="true">
                <field name="student"/>
            </panel-related>
        </panel-tabs>

    </form>
</object-views>