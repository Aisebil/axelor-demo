<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">
    <grid name="teacher-grid" title="教师" model="com.yyc.demo.db.Teacher">
        <field name="human" title="人类"/>
        <field name="school" title="学校"/>
    </grid>

    <form name="teacher-form" title="教师" model="com.yyc.demo.db.Teacher">
        <panel name="mainPanel" >
            <panel title="基本信息" colSpan="6" itemSpan="12">
                <field name="human"/>
                <field name="school"/>
            </panel>
        </panel>

    </form>
</object-views>